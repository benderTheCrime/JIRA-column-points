{
  "name": "JIRA Column Points",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Show Column Sums in JIRA Sprint Boards",
  "homepage_url": "https://www.spongecell.com",
  "icons": {},
  "permissions": [
    "tabs",
    "https://<your_organization>.atlassian.net/*",
    "http://<your_organization>.atlassian.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://<your_organization>.atlassian.net/*",
        "http://<your_organization>.atlassian.net/*"
      ],
      "js": [
        "src/js/int.js"
      ]
    }
  ]
}
